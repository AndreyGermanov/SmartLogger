{
  "cronjobs": {
    "enabled": true,
    "name": "statusReceiver",
    "port": 8910,
    "routes": {
      "post_status": {
        "name": "post_status",
        "url": "/status",
        "method": "POST"
      }
    }
  },
  "dashboard": {
    "enabled": true,
    "name": "dashboard",
    "port": 8080,
    "routes": {
      "get_cronjobs": {
        "name": "get_cronjobs",
        "url": "/cronjobs",
        "method": "GET"
      },
      "get_last_record": {
        "name": "get_last_record",
        "url": "/cronjobs/last_record/:cronjob_id"
      },
      "enable_cronjob": {
        "name": "enable_cronjob",
        "url": "/cronjobs/enable/:cronjob_id/:enable"
      }
    }
  },
  "flickr": {
    "enabled": true,
    "name": "flickr",
    "port": 8081,
    "routes": {
      "get_recent": {
        "name": "get_recent",
        "url": "/recent",
        "method": "GET"
      }
    }
  }
}